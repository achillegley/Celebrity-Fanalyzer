{
  "name": "celebrity-fanalyzer",
  "version": "1.3",
  "description": "Celebrities Over Analyzed",
  "productName": "Celebrity Fanalyzer",
  "private": true,
  "scripts": {
    "build": "quasar build --mode pwa",
    "dev": "quasar dev --mode pwa",
    "dev:test": "cross-env VITE_RELEASE_STAGE=testing quasar dev --mode pwa",
    "dev:e2e": "cross-env VITE_MODE=E2E quasar dev --mode pwa",
    "format": "prettier --write \"**/*.{js,vue,css,html,md,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.vue ./",
    "test": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test dev http://localhost:9200 cy:run",
    "emulators": "firebase emulators:start --project celebrityfanalizer --import emulatorData",
    "test:e2e": "cross-env NODE_ENV=test start-server-and-test http-get://localhost:9200 dev cy:run",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev --mode pwa\" http-get://localhost:9200 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@quasar/extras": "^1.16.3",
    "@sentry/tracing": "^7.50.0",
    "@sentry/vue": "^7.50.0",
    "dotenv": "^16.0.3",
    "echarts": "^5.4.2",
    "firebase": "^9.21.0",
    "jsdom": "^21.1.2",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "quasar": "^2.11.10",
    "sha1": "^1.1.1",
    "vue": "^3.2.47",
    "vue-echarts": "^6.5.5",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.15",
    "@quasar/app-vite": "^1.2.2",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.2",
    "@sentry/vite-plugin": "^0.4.0",
    "@vitest/coverage-istanbul": "^0.28.5",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "c8": "^7.13.0",
    "cypress": "^12.11.0",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^21.1.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "vitest": "^0.15.2",
    "vue-router-mock": "^0.2.0",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
